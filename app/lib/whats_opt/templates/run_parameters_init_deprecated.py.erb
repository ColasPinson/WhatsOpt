import numpy as np
from numpy import nan

def initialize(mda):
    <%- @mda.input_variables.each do |param| -%>
    mda['<%= param.name %>'] = <%= param.init_py_value %>
    <%- end -%>
    warning()
    return mda

def warning():
    print("*****************************************************************************************")
    print("* Warning: 'run_parameters_init.py' is deprecated and will be removed in future versions.")
    print("* Instead please use 'mda_init.py' ('run_parameters_init.py' renaming).")
    print("*****************************************************************************************")

if __name__ == "__main__":
    from tabulate import tabulate

    mda = {}
    initialize(mda)
    headers = ["parameter", "value"]
    data = [[k, v] for k, v in mda.items()]
    print(tabulate(data, headers))
    warning()
