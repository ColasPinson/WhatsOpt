import matplotlib.pyplot as plt

from SALib.analyze import morris as ma
from SALib.plotting import morris as mp

names = []
lower = []
upper = []
inputs = []
<% @input_varcases.each do |vc| %>
names.append("<%= vc.float_varname %>")
