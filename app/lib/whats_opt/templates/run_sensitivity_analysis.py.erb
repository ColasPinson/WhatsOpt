<% end %>
#bounds = zip(lower, upper)
inputs = np.vstack(inputs)
inputs = inputs.T
salib_pb = {
    'num_vars': len(names),
    'names': names,
    'groups': None,
    'bounds': None
}

<% @output_varcases.each do |vc| %>
