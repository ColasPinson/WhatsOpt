<%= form_for @notebook, :html => { :multipart => true, :role => "form" } do |f| %>

  <div class="form-group">
  	<%= f.label 'Jupyter ipynb file'%>
    <%= render "attachments/attachment_form", :f => f %>  
  </div>

  <div class="form-group">
    <%= f.submit "Submit", class: "btn btn-primary" %>
    <%= f.submit "Cancel", class: "btn", name: "cancel_button" %>
  </div>
  
<% end %>
