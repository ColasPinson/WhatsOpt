<h1>Multi Disciplinary Analyses</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mdas.each do |mda| %>
      <tr>
        <td><%= mda.name %></td>
        <td><%= link_to 'Show', mda %></td>
        <td><%= link_to 'Edit', edit_multi_disciplinary_analysis_path(mda) %></td>
        <td><%= link_to 'Destroy', mda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New MDA', new_multi_disciplinary_analysis_path, class: "btn btn-primary" %>
